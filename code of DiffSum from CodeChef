//https://www.codechef.com/problems/DIFFSUM
#include <iostream>
using namespace std;

int main() {
    int n1,n2;
    cin>>n1;
    cout<<"\n";
    cin>>n2;
    if(n1>n2)
    {
        int diff = n1- n2;
        cout<< diff;
    }
    else 
    {
        int sum= n1+n2;
        cout<<sum;
    }
	return 0;
}
