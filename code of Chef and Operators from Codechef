//https://www.codechef.com/problems/CHOPRT
#include <bits/stdc++.h>
using namespace std;

int main() {
	int t,a,b,i;
	cin>>t;
	for(i=0;i<t;i++)
	{
	    cin>>a>>b;
	    if(a>b)
	    {
	        cout<<">";
	    }
	    else if(a<b)
	    {
	        cout<<"<";
	    }
	    else
	    {
	        cout<<"=";
	    }
	    cout<<"\n";
	}
	return 0;
}
