//https://www.codechef.com/problems/REMISS
#include <iostream>
using namespace std;

int main() {
	int t,a,b,i,max;
	cin>>t;
	for(i=0;i<t;i++)
	{
	    cin>>a>>b;
	    if(a>b)
	    {
	        max=a+b;
	        cout<<a<<" "<<max<<'\n';
	    }
	    else
	    {
	        max=a+b;
	        cout<<b<<" "<<max<<'\n';
	    }
	}
	return 0;
}
