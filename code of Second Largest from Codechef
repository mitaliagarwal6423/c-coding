//https://www.codechef.com/problems/FLOW017
#include <bits/stdc++.h>
using namespace std;

int main() {
	int t,i,n,j,arr[3];
	cin>>t;
	for(i=0;i<t;i++)
	{
	    for(j=0;j<3;j++)
	    {
	        cin>>arr[j];
	    }
	    n=sizeof(arr)/sizeof(arr[0]);
	    sort(arr,arr+n);
	    cout<<arr[1]<<"\n";
	}
	return 0;
}
