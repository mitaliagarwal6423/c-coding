//https://www.codechef.com/problems/FLOW013
#include <iostream>
using namespace std;

int main() {
	int i,t,a,b,c,sum=0;
	cin>>t;
	for(i=0;i<t;i++)
	{
	    cin>>a>>b>>c;
	    sum = a+b+c;
	    if(sum == 180)
	    {
	        cout<<"YES";
	    }
	    else
	    {
	        cout<<"NO";
	    }
	    cout<<'\n';
	}
	return 0;
}
